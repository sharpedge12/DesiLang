# FizzBuzz

fizzBuzz: 1, 20

fizzBuzz :: {start: ank, end: ank}: (
	
	i: in.start | i <= in.end | i: i+1 @ (
		
		# use conditionals to print the right thing
		
		i % 3 = 0 && i % 5 = 0 ?
			print: "FizzBuzz"
		|
		i % 3 = 0 ?
			print: "Fizz"
		|
		i % 5 = 0 ?
			print: "Buzz"
		|
			print: i
	)
)


