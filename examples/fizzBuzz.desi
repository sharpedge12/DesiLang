# FizzBuzz

fizzBuzzKaFunction: 1, 20

fizzBuzzKaFunction :: {pehla: ank, aakhri: ank}: (
	
	i: in.pehla | i <= in.aakhri | i: i+1 @ (
		
		i % 3 = 0 && i % 5 = 0 ?
			print: "FizzBuzz"
		|
		i % 3 = 0 ?
			print: "Fizz"
		|
		i % 5 = 0 ?
			print: "Buzz"
		|
			print: i
	)
)


